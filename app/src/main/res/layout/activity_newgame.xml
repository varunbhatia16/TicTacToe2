    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/symtitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="YOU"
            android:layout_marginStart="15dp"
            android:paddingStart="15dp"
            android:textStyle="bold"/>

        <ImageView
            android:id="@+id/sym"
            android:layout_width="80dip"
            android:layout_height="40dip"
            android:layout_alignLeft="@+id/symtitle"
            android:layout_below="@+id/symtitle"
            />

        <TextView
            android:id="@+id/symtitle1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/sym"
            android:text="COMPUTER"
            android:textStyle="bold"/>

        <ImageView
            android:id="@+id/sym1"
            android:layout_width="80dip"
            android:layout_height="40dip"
            android:layout_alignLeft="@+id/symtitle1"
            android:layout_below="@+id/symtitle1"
            android:layout_marginRight="60dp"
            />

        <Chronometer
            android:id="@+id/mychronometer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/sym1"/>

    </RelativeLayout>
