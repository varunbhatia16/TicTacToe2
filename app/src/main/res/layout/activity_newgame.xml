    <ImageButton android:id="@+id/b3"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="0dip"
            android:layout_marginTop="60dip"
            android:layout_marginRight="10dip"
            android:scaleType="fitXY"
            android:tag="3"

            />
        <ImageButton android:id="@+id/b2"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:layout_toLeftOf="@id/b3"
            android:layout_marginTop="60dip"
            android:scaleType="fitXY"
            android:tag="2"
            />
        <ImageButton android:id="@+id/b1"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:layout_toLeftOf="@id/b2"
            android:layout_marginTop="60dip"
            android:scaleType="fitXY"
            android:tag="1"
            />

        <ImageButton android:id="@+id/b6"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="0dip"
            android:layout_below="@id/b3"
            android:layout_marginRight="10dip"
            android:scaleType="fitXY"
            android:tag="6"
            />
        <ImageButton android:id="@+id/b5"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:layout_toLeftOf="@id/b6"
            android:layout_below="@id/b2"
            android:scaleType="fitXY"
            android:tag="5"

            />
        <ImageButton android:id="@+id/b4"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:layout_toLeftOf="@id/b5"
            android:layout_below="@id/b1"
            android:scaleType="fitXY"
            android:tag="4"
            />

        <ImageButton android:id="@+id/b9"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="0dip"
            android:layout_below="@id/b6"
            android:layout_marginRight="10dip"
            android:scaleType="fitXY"
            android:tag="9"
            />
        <ImageButton android:id="@+id/b8"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:layout_toLeftOf="@id/b9"
            android:layout_below="@id/b5"
            android:scaleType="fitXY"
            android:tag="8"
            />
        <ImageButton android:id="@+id/b7"
            android:layout_width="100dip"
            android:layout_height="100dip"
            android:layout_toLeftOf="@id/b8"
            android:layout_below="@id/b4"
            android:scaleType="fitXY"
            android:tag="7"

            />

    <TextView android:id="@+id/scoreboard"
        android:layout_below="@id/b9"
        android:layout_width="300dip"
        android:layout_height="50dip"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dip"
        android:layout_marginTop="10dip"
        android:padding="15dip"

        />

